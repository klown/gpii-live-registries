{
  "name": "TalkBack Screen Reader",
  "gradeNames": ["gpii.solutions.android.base"],
  "settingsHandlers": {
    "configuration": {
      "type": "gpii.settingsHandlers.noSettings",
      "capabilities": [
        "http://registry\\.gpii\\.net/common/screenReaderTTSEnabled"
      ]
    }
  },
  "start": [
    {
      "type": "gpii.androidActivityManager.startTalkback"
    }
  ],
  "stop": [
    {
      "type": "gpii.androidActivityManager.stopTalkback"
    }
  ]
}